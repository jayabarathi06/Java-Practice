import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long n = sc.nextLong();
        int count = 0;
        int found= 0; // 0 means not palindrome yet
         while (count < 5) {
            count++;
          // reverse the number
            long temp = n;
            long rev = 0;
            while (temp > 0) {
                rev = rev * 10 + temp % 10;
                temp = temp / 10;
            }

            n = n + rev; // add reversed number

            // check palindrome (without boolean)
            long num = n;
            long check = 0;
            long copy = num;
            while (num > 0) {
                check = check * 10 + num % 10;
                num = num / 10;
            }

            if (check == copy) {
                found = 1;
                break;
            }
        }

        if (found == 1) {
            System.out.println("Lucky Number");
            System.out.println("Iterations: " + count);
        } else {
            System.out.println("Unlucky Number");
            System.out.println("Iterations: 5");
        }
    }
}
