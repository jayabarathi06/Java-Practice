import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		for(int i=0;i<s.length();i++){
		    char ch=s.charAt(i);//check the character one by one
		    if(ch>96 && ch<123)
          {
            int c=ch-32;//covert and store the integer value of ascii value
          
            System.out.print( (char) c);//and then convert integer to character
          }
           
		}
	}

}
